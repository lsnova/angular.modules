{
  "name": "angularmodules",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "libs:build": "cd projects/lsn-libs && ng build lsn-libs --configuration production",
    "libs:build-watch": "cd projects/lsn-libs && ng build lsn-libs --watch",
    "libs:test": "ng test lsn-libs",
    "package": "npm run libs:build && rsync -a dist/lsn-libs/* ../",
    "postinstall": "npm run libs:build && npm install -S file:dist/lsn-libs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "~18.2.13",
    "@angular/compiler": "~18.2.13",
    "@angular/core": "~18.2.13",
    "@angular/forms": "~18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "~18.2.13",
    "@angular/platform-browser-dynamic": "~18.2.13",
    "@angular/router": "~18.2.13",
    "@lsnova/angularmodules": "file:dist/lsn-libs",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "~18.2.13",
    "@angular/language-service": "~18.2.13",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~6.0.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^18.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.5.4"
  }
}
