{
  "name": "angularmodules",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "libs:build": "cd projects/lsn-libs && ng build lsn-libs",
    "libs:build-watch": "cd projects/lsn-libs && ng build lsn-libs --watch",
    "libs:test": "ng test lsn-libs",
    "package": "npm run libs:build && rsync -a dist/lsn-libs/* ../",
    "postinstall": "npm run libs:build && npm install -S file:dist/lsn-libs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@lsnova/angularmodules": "file:dist/lsn-libs",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.38.1",
    "tslib": "^1.13.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
