{
  "name": "angularmodules",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "libs:build": "cd projects/lsn-libs && ng build lsn-libs --configuration production",
    "libs:build-watch": "cd projects/lsn-libs && ng build lsn-libs --watch",
    "libs:test": "ng test lsn-libs",
    "package": "npm run libs:build && rsync -a dist/lsn-libs/* ../",
    "postinstall": "npm run libs:build && npm install -S file:dist/lsn-libs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@lsnova/angularmodules": "file:dist/lsn-libs",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular-devkit/build-ng-packagr": "^0.1002.4",
    "@angular/cli": "^12.2.17",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.39.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
